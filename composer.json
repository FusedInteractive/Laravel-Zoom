{
    "name": "fusedinteractive/laravel-zoom",
    "description": "Integrates Zoom video meeting system with Laravel",
    "keywords": ["laravel","zoom","video","meeting","conference","conferencing"],
    "homepage": "https://github.com/FusedInteractive/laravel-zoom",
    "license": "MIT",
    "authors": [
        {
            "name": "Doug Sisk",
            "homepage": "http://dougsisk.com"
        }
    ],
    "support": {
        "issues": "https://github.com/FusedInteractive/laravel-zoom/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~5.0|~6.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Fused\\Zoom\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}